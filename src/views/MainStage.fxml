<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.effect.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="350.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="views.MainStageController">
   <children>
      <ImageView fx:id="logo" fitHeight="212.0" fitWidth="283.0" layoutX="48.0" layoutY="66.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../css/picture/Octocat.png" />
         </image>
      </ImageView>
      <Label layoutX="75.0" layoutY="22.0" text="Elevator's Prototype">
         <font>
            <Font name="System Bold" size="21.0" />
         </font>
      </Label>
      <GridPane layoutX="82.0" layoutY="300.0" prefHeight="150.0" prefWidth="187.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="Setting Up!" GridPane.halignment="CENTER" GridPane.valignment="CENTER">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
            </Label>
            <RadioButton fx:id="random" mnemonicParsing="false" text="Auto mode." GridPane.halignment="CENTER" GridPane.rowIndex="1" GridPane.valignment="CENTER" />
            <Button fx:id="start" mnemonicParsing="false" onAction="#startAction" prefHeight="25.0" prefWidth="68.0" text="Start" GridPane.halignment="CENTER" GridPane.rowIndex="3" GridPane.valignment="CENTER" />
            <RadioButton fx:id="control" mnemonicParsing="false" text="Control mode." GridPane.halignment="CENTER" GridPane.rowIndex="2" GridPane.valignment="CENTER" />
         </children>
      </GridPane>
      <Label fx:id="check" layoutX="161.0" layoutY="456.0" text="Label" />
   </children>
</AnchorPane>
